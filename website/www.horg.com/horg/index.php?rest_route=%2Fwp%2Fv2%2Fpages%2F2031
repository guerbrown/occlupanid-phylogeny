ex.php?rest_route=%2Fwp%2Fv2%2Fcomments&post=2031"}],"version-history":[{"count":5,"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=\/wp\/v2\/pages\/2031\/revisions"}],"predecessor-version":[{"id":3124,"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=\/wp\/v2\/pages\/2031\/revisions\/3124"}],"up":[{"embeddable":true,"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=\/wp\/v2\/pages\/377"}],"wp:featuredmedia":[{"embeddable":true,"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=\/wp\/v2\/media\/2032"}],"wp:attachment":[{"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=%2Fwp%2Fv2%2Fmedia&parent=2031"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}