ex.php?rest_route=%2Fwp%2Fv2%2Fcomments&post=2398"}],"version-history":[{"count":1,"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=\/wp\/v2\/pages\/2398\/revisions"}],"predecessor-version":[{"id":2400,"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=\/wp\/v2\/pages\/2398\/revisions\/2400"}],"up":[{"embeddable":true,"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=\/wp\/v2\/pages\/790"}],"wp:featuredmedia":[{"embeddable":true,"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=\/wp\/v2\/media\/2399"}],"wp:attachment":[{"href":"http:\/\/www.horg.com\/horg\/index.php?rest_route=%2Fwp%2Fv2%2Fmedia&parent=2398"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}